{
  "author": "Florian Keller <github@floriankeller.de>",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "json5": "2.1.0",
    "jsonabc": "2.3.1",
    "mdi-material-ui": "5.14.0",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "devDependencies": {
    "@ffflorian/prettier-config": "0.0.5",
    "@ffflorian/tslint-config": "0.5.0",
    "@types/jest": "24.0.17",
    "@types/json5": "0.0.30",
    "@types/jsonabc": "2.3.1",
    "@types/node": "~12",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.8.5",
    "gh-pages": "2.1.1",
    "husky": "3.0.3",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "react-scripts": "3.0.1",
    "rimraf": "2.6.3",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ]
  },
  "name": "sortjson.com",
  "prettier": "@ffflorian/prettier-config",
  "private": true,
  "scripts": {
    "build:site": "react-scripts build",
    "clear": "rimraf build",
    "deploy": "gh-pages -t -d build -m \"Publish v$npm_package_version\"",
    "dist": "yarn clear && yarn build:site",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --project tsconfig.json \"**/*.{ts,tsx}\"",
    "prettier": "prettier --ignore-path .gitignore \"src/*.{ts,tsx}\"",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "version": "1.5.6"
}
