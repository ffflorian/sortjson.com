{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "3.9.4",
    "@material-ui/icons": "3.0.2",
    "json5": "2.2.0",
    "jsonabc": "2.3.1",
    "mdi-material-ui": "5.33.0",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@ffflorian/eslint-config": "0.8.1",
    "@ffflorian/prettier-config": "0.1.2",
    "@types/jest": "26.0.22",
    "@types/jsonabc": "2.3.1",
    "@types/node": "~14",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "babel-eslint": "10.1.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jasmine": "4.1.2",
    "eslint-plugin-jsdoc": "32.3.0",
    "eslint-plugin-no-unsanitized": "3.1.4",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-typescript-sort-keys": "1.5.0",
    "eslint-plugin-unused-imports": "1.1.1",
    "gh-pages": "3.1.0",
    "husky": "5.2.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "react-scripts": "4.0.3",
    "rimraf": "3.0.2",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "license": "GPL-3.0",
  "lint-staged": {
    "*.{json,md,css}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "name": "sortjson.com",
  "prettier": "@ffflorian/prettier-config",
  "private": true,
  "scripts": {
    "build:site": "react-scripts build",
    "clear": "rimraf build",
    "deploy": "gh-pages -t -d build -m \"Publish v$npm_package_version\"",
    "dist": "yarn clear && yarn build:site",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "eslint --ignore-path .gitignore --ext .ts,.tsx src/",
    "prettier": "prettier --ignore-path .gitignore \"src/*.{ts,tsx}\"",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "version": "1.5.6"
}
