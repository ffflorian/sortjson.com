    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "gh-pages": "4.0.0",
    "prettier": "2.8.1",
    "react-scripts": "4.0.3",
    "rimraf": "3.0.2",
    "typescript": "4.9.3"
